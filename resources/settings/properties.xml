<resources>
	<properties>
		<property id="isDist" type="number">0</property>
		<property id="rollingDuration" type="string">100</property>
	</properties>
    <settings>
		<setting propertyKey="@Properties.isDist" title="@Strings.isDistTitle">
			<settingConfig type="list">
				<listEntry value="0">@Strings.dist</listEntry>
				<listEntry value="1">@Strings.time</listEntry>
			</settingConfig>
		</setting>
		<setting propertyKey="@Properties.rollingDuration" title="@Strings.rollingDurationTitle">
			<settingConfig type="alphaNumeric" errorMessage="@Strings.enterNumber"></settingConfig>
		</setting>
	</settings>
</resources>